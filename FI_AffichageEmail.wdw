  procedures :
   -
     name : AfficheDétailsMessage
     procedure_id : 2281810833748176822
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //AfficheDétailsMessage ( [<gpclEmail> est cEmail dynamique])
      //
      // Paramètres :
      //	gpclEmail (cEmail dynamique - valeur par défaut=0) : email à afficher
      // Valeur de retour :
      // 	Aucune
      
      procédure AfficheDétailsMessage(pclEmail est un cEmail dynamique = null)
      
      
      si pclEmail <> null alors 
      
      	si pclEmail.ContenuEstSynchro=faux ALORS
      		// Si seules les entêtes ont été récupérées (IMAP et mail pas encore affiché), il faut lire le corps
      		dbgAssertion(pclEmail.IDCompte<>0)
      		pclCompte est un cCompte dynamique = cCompte::s_GetCompte(pclEmail.IDCompte)
      		pclCompte.emailRecupereContenu(pclEmail)
      	FIN
      
      	gclEmailAffiché = pclEmail
      	
      	// Expéditeur
      	LIB_EXPEDITEUR_DONNEES		= cEmail::s_nomExtrait(pclEmail.Expediteur)
      	
      	// Destinataire
      	LIB_DESTINATAIRE_DONNEES	= TableauVersChaîne(pclEmail.Destinataire," ")
      	
      	LIB_SUJET_DONNEE			= pclEmail.Sujet
      	
      	// Date Heure
      	LIB_DATE_HEURE				= DateVersChaîne(pclEmail.DateHeureReception..PartieDate, <§$0000§>) + <§$0001§> + HeureVersChaîne(pclEmail.DateHeureReception..PartieHeure, "HH:MM")
      		
      	si pclEmail.html alors
      		HTM_AffichageEmail	= pclEmail.genereFichierTempPourAffichage()
      	sinon
      		HTM_AffichageEmail..Body	= "<pre>"+pclEmail.Contenutexte+"</pre>"
      	fin
      	
      	BTN_AJOUTER_DESTINATAIRE..Visible = faux
      	pour tout sDest de pclEmail.Destinataire
      		si Contact_Existe(cemail::s_emailExtrait(sDest))=faux alors
      			BTN_AJOUTER_DESTINATAIRE..Visible = vrai 
      			sortir
      		fin
      	FIN
      	BTN_AJOUTER_EXPEDITEUR..Visible = (Contact_Existe(cEmail::s_emailExtrait(pclEmail.Expediteur))=faux)
      	
      	
      	// Variable
      	tabPJ		est un tableau de cPieceJointe dynamique
      	sFichierTmp	est une chaîne	= fFichierTemp("TMPIMG",COL_WDMail.gsRepertoireTemp)
      	
      	
      	// Pièces jointes
      	SI gclEmailAffiché.NbPieceJointe > 0 ALORS
      		// Récupère les pièces jointes
      		tabPJ = gclEmailAffiché.getPiecesJointes()
      		
      		// Récupère la première PJ pour afficher une image
      		IMG_APERCUS = imageSelonExtension(fExtraitChemin(tabPJ[1].NomFichier,fExtension)) 
      		
      		// Recherche la première image
      		POUR TOUT pclPJ DE tabPJ
      			SI cPieceJointe::imageEstGerée(pclPJ.NomFichier) = Vrai ALORS
      				// Sauvegarde temporaire
      				fSauvebuffer(sFichierTmp,pclPJ.Contenu)
      				IMG_PREVIEW				= sFichierTmp
      				IMG_PREVIEW..Visible	= Vrai
      				SORTIR
      			FIN
      		FIN
      		
      		// Affiche les informations
      		BTN_PIECE_JOINTE..Libellé	= ChaîneConstruit(<§$0011§>, gclEmailAffiché.NbPieceJointe, TailleVersChaîne(gclEmailAffiché.TailleTotale))
      		BTN_PIECE_JOINTE..Visible	= Vrai
      		IMG_APERCUS..Visible		= Vrai
      		// Sinon pas de PJ pour cet email
      	FIN
      	
      	MoiMême..Plan = 1
      	
      sinon
      	
      	MoiMême..Plan = 2
      	
      fin
      
      
      
     type : 458752
   -
     name : imageSelonExtension
     procedure_id : 2289235599235168311
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //[ <Résultat> = ] imageSelonExtension (<sExtension> est chaîne)
      //
      // Paramètres :
      //	sExtension (chaîne) : <indiquez ici le rôle de sExtension>
      // Valeur de retour :
      // 	variant : // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCÉDURE imageSelonExtension(LOCAL sExtension est une chaîne)
      
      SELON Majuscule(sExtension)
      	CAS ".XML":
      		RENVOYER IMG_XML
      	CAS ".DOC", ".DOCX":
      		RENVOYER IMG_WORD
      	CAS ".XLS", ".XLSX":
      		RENVOYER IMG_EXCEL
      	CAS ".PDF":
      		RENVOYER IMG_PDF
      	CAS ".JPG", ".BMP", ".JPEG", ".PNG", ".TIFF", "GIF":
      		RENVOYER IMG_IMAGE
      	CAS ".MP3", ".WAV", ".OGG":
      		RENVOYER IMG_MUSIQUE
      	CAS ".AVI", ".WMV", ".DIVX":
      		RENVOYER IMG_VIDEO
      	AUTRE CAS :
      		RENVOYER IMG_AUTRE
      FIN
      
     type : 458752
   -
     name : FermeVoletDynamique
     procedure_id : 2295946567175980780
     type_code : 14
     code : |1-
      procédure FermeVoletDynamique()
      
      VoletEnCours est un Champ
      VoletEnCours <- VoletDuChamp(MoiMême)
      SI VoletEnCours..Alias[="ALIAS_" ALORS
      	OngletFerme(FEN_Principale.ONG_WDMail)
      FIN
     type : 458752
