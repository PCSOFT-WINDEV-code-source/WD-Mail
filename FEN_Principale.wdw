  procedures :
   -
     name : actualiseListeDossier
     procedure_id : 1967013820972204429
     type_code : 14
     code : |1+
      // Résumé : Actualise les données affichées à la fenêtre
      // Syntaxe :
      //actualiseListeDossier ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      
      PROCÉDURE actualiseListeDossier()
      
      // Variables
      taListeCompte    est un tableau associatif (*, *, wlEntier_8) de cCompte dynamique
      tabFils          est un tableau de cDossier dynamique
      pclCompte        est un cCompte dynamique
      pclDossier       est un cDossier dynamique
      sImage           est une chaîne
      nIndAjout        est un entier
      
      // Mémorise la sélection
      nTypeSelectionDossier est un entier // 0 = pas de sélection, 1 = compte sélectionné, 2 = dossier sélectionné
      nIdSelection est un entier sur 8 octets
      si TableSelectOccurrence(TABLEH_COMPTES)>0 alors
      	si TABLEH_COMPTES.COL_IDDOSSIER=0 ALORS
      		nIdSelection = TABLEH_COMPTES.COL_IDCOMPTE
      		nTypeSelectionDossier = 1
      	sinon
      		nIdSelection = TABLEH_COMPTES.COL_IDDOSSIER
      		nTypeSelectionDossier = 2
      	FIN
      sinon
      	nTypeSelectionDossier = 0
      FIN
      
      // Réinitialisation
      TableSupprimeTout(TABLEH_COMPTES)
      
      // Affiche la liste des comptes
      taListeCompte = cCompte::s_getListeCompte()
      
      // Aucun compte ?
      SI taListeCompte..Occurrence = 0 ALORS
      	SI Ouvre(FEN_CompteCreation) = Vrai ALORS
      		taListeCompte = cCompte::s_getListeCompte()
      		emailReleverTout()
      		retour
      	SINON
      		Ferme()
      	FIN
      FIN
      
      // Ajoute dans la table hiérarchique
      POUR TOUT ELEMENT pclCompte DE taListeCompte
      	// Ajoute le compte dans la table hiérarchique
      	TABLEH_COMPTES..ImageDéroulée = IMG_COMPTE
      	TABLEH_COMPTES..ImageEnroulée = IMG_COMPTE
      	TableAjouteFils(TABLEH_COMPTES, Null, pclCompte.Nom, pclCompte.IDCompte, cDossier::DOSSIER_AUCUN_PARENT, Vrai,0)
      	
      	// Récupère la liste des dossiers du compte
      	tabFils = pclCompte.getDossiers()
      	
      	si tabFils..Occurrence>0 alors
      		
      		// Ajoute les dossiers dans la table hiérarchique
      		POUR TOUT ELEMENT pclDossier DE tabFils
      			si pclDossier.bDossierAIgnorer() ALORS continuer
      			//  Récupère l'image associé au type de fichier
      			sImage = pclDossier.image()
      			
      			// Ajoute dans la table hiérarchique
      			TABLEH_COMPTES..ImageDéroulée = sImage
      			TABLEH_COMPTES..ImageEnroulée = sImage
      			
      			// Ajoute les dossiers fils
      			sCheminDossierParentDansTableH est une chaine = Remplace(pclDossier.cheminDossierParent(),"/",tab)
      			sCheminDossierParentDansTableH = remplace(sCheminDossierParentDansTableH,"[Gmail]","")
      			
      			nIndAjout = TableAjouteFils(TABLEH_COMPTES, pclCompte.Nom + [TAB] + sCheminDossierParentDansTableH, pclDossier.Libelle , pclCompte.IDCompte, pclDossier.IDDossier, Faux, pclDossier.NbNonLus)
      			ChangeNombreNonLusTableHierarchique(nIndAjout,pclDossier.NbNonLus)
      		FIN
      	fin
      	
      FIN
      
      actualiseTitre()
      
      // Restaure la sélection
      selon nTypeSelectionDossier
      	cas 1 // Compte
      		nLigne est un entier = TableCherche(COL_IDCOMPTE,nIdSelection)
      		SI nLigne>0 ALORS TableSelectPlus(TABLEH_COMPTES,nLigne)
      	cas 2 // Dossier
      		nLigne est un entier = TableCherche(COL_IDDOSSIER,nIdSelection)
      		SI nLigne>0 ALORS TableSelectPlus(TABLEH_COMPTES,nLigne)
      	AUTRE CAS
      		// Rien
      fin
      
      // Action de sélection
      selectionTableHierarchique()
      
