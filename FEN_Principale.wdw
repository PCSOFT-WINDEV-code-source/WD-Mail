   -
     name : selectionTableHierarchique
     internal_properties : CAAAAAgAAABK+wMsO9TiBl+ksfDf5buElP+yI25FSQyZuEokcnnsv4Zj0UW7l/98CQKzkdDYDvcEx7GQGgyzNqHZdhBl4v3wldOHlCi2oaBZaphLxSwZjtMYk6dIQ9Slh+8Mk6qZ2hEcQWduGK4x4KGsbnWR
     procedure_id : 1967500737082479315
     type_code : 14
     code : |1+
      // Résumé : Appelé lorsqu'une sélection est effectuée dans la table hiérarchique
      // Syntaxe :
      //selectionTableHierarchique ( [<bSynchro> est booléen])
      //
      // Paramètres :
      // 	bSynchro (booléen - valeur par défaut=1) : <indiquez ici le rôle de bSynchro>
      // Valeur de retour :
      // 	Aucune
      
      PROCÉDURE selectionTableHierarchique( bSynchro est un booleen = faux )
      
      // Variables
      pclCompte		est un cCompte dynamique	= ccompte::s_getCompte(TABLEH_COMPTES.COL_IDCOMPTE)
      si pclCompte=Null ALORS
      	retour
      FIN
      
      actualiseTitre()
      
      nLigneTH est un entier = TABLEH_COMPTES
      pclReception	est un cDossier dynamique	= pclCompte.getDossierReception()
      
      // Ré-initialise
      SI optionExiste(MENU_COMPTE,"OPTION_VIDER") ALORS
      	MenuSupprime("MENU_COMPTE.OPTION_VIDER")
      FIN
      
      // Change de plan si on a sélectionné la racine d'un compte
      pclDossier est un cDossier dynamique = null
      SI TABLEH_COMPTES.COL_RACINE = Vrai ALORS
      	// Modifie l'affichage dans le menu
      	MENU_COMPTE.OPT_DOSSIER_SUPPRIME..Libellé = <§$0011§>
      	TABLE_EMAIL..MenuContextuel = MenuContextuelDésactivé
      	TableSupprimeTout(TABLE_EMAIL)
      	CFI_Message.ChangeFenêtreSource(FI_AffichageEmail)
      	
      SINON
      	// Modifie l'affichage dans le menu
      	MENU_COMPTE.OPT_DOSSIER_SUPPRIME..Libellé = <§$0010§>
      	
      	// Affiche les emails associés au dossier (si ça en est un)
      	SI TABLEH_COMPTES.COL_IDDOSSIER <> cDossier::DOSSIER_AUCUN_PARENT ALORS
      		pclDossier = pclCompte.getDossier(TABLEH_COMPTES.COL_IDDOSSIER)
      		si pclCompte.TypeReception = cCompte::RECEPTION_IMAP _et_ bSynchro alors
      			GR_OPERATION_EN_COURS..visible = vrai
      			COL_WDMail.gbDemandeAbandonOperation = Faux
      			si pclDossier.SynchroAFaire() alors
      				arreteNotifieur()
      				pclCompte.CallbackAvancement = callbackAvancementOperation
      				pclCompte.dossierSynchroniseIMAP(pclDossier)
      				ChangeNombreNonLusTableHierarchique(nLigneTH,pclDossier.NbNonLus)
      				demarreNotifieur()
      			fin
      			GR_OPERATION_EN_COURS..visible = faux
      		fin
      		actualiseListeEmail(TABLEH_COMPTES[nLigneTH].COL_IDCOMPTE, TABLEH_COMPTES[nLigneTH].COL_IDDOSSIER)
      	FIN	
      	
      	// Sélectionne le premier email de la table par défaut
      	SI TABLE_EMAIL..Occurrence > 0 ALORS
      		timerDoubleClic()
      	SINON
      		CFI_Message.ChangeFenêtreSource(FI_AffichageEmail)
      	FIN
      	
      	// Propose de vider
      	SI PAS optionExiste(MENU_COMPTE,"OPTION_VIDER") ALORS
      		MenuAjouteOption(MENU_COMPTE,"OPTION_VIDER", <§$000d§>, viderDossier, TABLEH_COMPTES[nLigneTH].COL_IDDOSSIER)
      	FIN
      	
      	// Change le menu de la table selon qu'on a sélectionné le dossier des brouillons ou un autre
      	si pclDossier=null _ou_ pclDossier.Type=cDossier.DOSSIER_BROUILLON alors
      		TABLE_EMAIL..MenuContextuel = MenuContextuelDésactivé
      	SINON
      		TABLE_EMAIL..MenuContextuel = MENU_EMAIL
      	fin
      FIN
      
      // Donne le focus à la table
      DonneFocus(TABLE_EMAIL)
      
