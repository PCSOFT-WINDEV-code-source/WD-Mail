   -
     name : changeNombreNonLusTableHierarchique
     procedure_id : 1155520514070789021
     type_code : 14
     code : |1+
      procédure changeNombreNonLusTableHierarchique( nIndice est un entier, nNbNonLus est un entier )
      
      TABLEH_COMPTES[nIndice].COL_NB_NON_LUS = nNbNonLus
      TABLEH_COMPTES[nIndice]..PoliceGras = (nNbNonLus>0)
