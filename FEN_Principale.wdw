      //suppressionTableHierarchique ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //// Traitement automatique des exceptions :   exécuter le bloc de code CAS EXCEPTION:
      //
      
      PROCÉDURE suppressionTableHierarchique()
      
      arreteNotifieur()
      
      // Variables
      pclCompte est un cCompte dynamique = cCompte::s_getCompte(TABLEH_COMPTES.COL_IDCOMPTE)
      
      // Vérification
      dbgAssertion(pclCompte <> Null,"pclCompte == Null")
      
      // Suppression de dossier ou de compte ?
      SI TABLEH_COMPTES.COL_RACINE = Vrai ALORS
      	
      	// Supprime le compte ?
      	//1 : &Supprimer
      	//2 : &Ne pas supprimer
      	SELON Dialogue(<§?1B4C741F01CFB21B§>)
      		// &Supprimer
      		CAS 1
      			ccompte::s_Supprime(TABLEH_COMPTES.COL_IDCOMPTE)
      			
      			actualiseListeDossier()	
      		// &Ne pas supprimer
      		CAS 2
      			
      	FIN
      SINON
      	// Supprime le dossier ?
      	//1 : &Supprimer
      	//2 : &Ne pas supprimer
      	SELON Dialogue(<§?1B4C607100ECD2E2§>)
      		// &Supprimer
      		CAS 1
      			// Supprime le dossier
      			pclCompte.dossierSupprime(TABLEH_COMPTES.COL_IDDOSSIER)	
      			
      			// Actualise les informations
      			actualiseListeDossier()
      		// &Ne pas supprimer
      		CAS 2
      			
      	FIN	
      FIN
      
      demarreNotifieur()
      
      CAS EXCEPTION:
      	Erreur(ExceptionInfo(errMessage))
