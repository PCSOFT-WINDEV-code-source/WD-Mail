      // Résumé : Affiche les emails du dossier donné en paramètre
      // Syntaxe :
      //actualiseListeEmail (<nIDCompte> est entier sur 8 octets, <nIDDossier> est entier sur 8 octets)
      //
      // Paramètres :
      //	nIDCompte (entier sur 8 octets) : <indiquez ici le rôle de nIDCompte>
      //	nIDDossier (entier sur 8 octets) : <indiquez ici le rôle de nIDDossier>
      // Valeur de retour :
      // 	Aucune
      
      PROCÉDURE actualiseListeEmail(LOCAL nIDCompte est un entier sur 8, LOCAL nIDDossier est un entier sur 8)
      
      // Variables
      pclDossier   est un cDossier dynamique = Null
      pclCompte    est un cCompte dynamique  = Null
      sImagePJ     est une chaîne
      sImageLu     est une chaîne
      sEmails      est une chaîne
      taListeEmail est un tableau associatif (*, *, wlEntier_8) de cEmail dynamique
      nIndice      est un entier
      
      // Initialise
      nIndice = TABLE_EMAIL
      TableSupprimeTout(TABLE_EMAIL)
      
      // Récupère le compte
      pclCompte  = cCompte::s_getCompte(nIDCompte)
      
      // Récupère le dossier du compte
      pclDossier = pclCompte.getDossier(nIDDossier)
      
      // Change le nom de la colonne adresse
      SI pclDossier.Type = cDossier::DOSSIER_ENVOYE ALORS
      	TABLE_EMAIL.COL_ADRESSE..Libellé = <§$0014§>
      SINON
      	TABLE_EMAIL.COL_ADRESSE..Libellé = <§$0015§>
      FIN
      
      // Affiche les emails
      taListeEmail = pclDossier.emailsContenus()
      POUR TOUT ELEMENT pclEmail DE taListeEmail
      	// Images dans la table
      	sImagePJ = (pclEmail.NbPieceJointe > 0) ? IMG_PIECE_JOINTE SINON ""
      	sImageLu = (pclEmail.Lu = Faux) ? IMG_NON_LU SINON ""
      	
      	// Définit l'adresse à afficher (destinataire ou expéditeur)
      	SI pclDossier.Type = cDossier::DOSSIER_ENVOYE ALORS
      		sEmails = TableauVersChaîne(pclEmail.Destinataire,", ")
      	SINON
      		sEmails = cEmail::s_nomExtrait(pclEmail.Expediteur)
      	FIN
      
      	// Insère la ligne
      	nInd est un entier = TABLE_EMAIL.AjouteLigne( sImagePJ, pclEmail.Sujet,sEmails,pclEmail.DateHeureReception, DateVersChaîne(pclEmail.DateHeureReception..partieDate,"JJJ JJ/MM") + rc +  HeureVersChaîne(pclEmail.DateHeureReception..partieheure,"HH:mm"), pclEmail.DateHeureReception..PartieHeure, pclEmail.IDEmail, pclEmail.Lu, pclEmail.Contenuhtml)
      	TABLE_EMAIL.COL_SUJET[nInd].LIB_Expéditeur = sEmails
      	TABLE_EMAIL.COL_SUJET[nInd].LIB_Expéditeur..Bulle = sEmails
      	TABLE_EMAIL.COL_SUJET[nInd].LIB_Sujet..Bulle = pclEmail.Sujet
      	TABLE_EMAIL.COL_SUJET[nInd].img_Lu = sImageLu
      FIN
      
      // Trie la table par date
      TableTrie(TABLE_EMAIL, "-" + TABLE_EMAIL.COL_DATE..Nom)
      
      // Il y a des emails ?
      SI TABLE_EMAIL..Occurrence > 0 ALORS
      	SI nIndice > TABLE_EMAIL..Occurrence ALORS
      		TableSelectPlus(TABLE_EMAIL,TABLE_EMAIL..Occurrence)
      	SINON SI nIndice <> -1 ALORS
      		TABLE_EMAIL = nIndice
      	SINON
      		TABLE_EMAIL = 1
      	FIN	
      FIN
