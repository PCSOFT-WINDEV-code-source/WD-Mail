     name : emailChangeEtatSelection
     procedure_id : 2302968920249138301
     type_code : 14
     code : |1-
      procédure emailChangeEtatSelection( bLu est un booleen )
      
      nNbChangements est un entier
      
      POUR TOUTE LIGNE SELECTIONNÉE DE TABLE_EMAIL
      	pclCompte est un cCompte dynamique = cCompte.s_GetCompte(TABLEH_COMPTES.COL_IDCOMPTE)
      	pclCompte.getEmail(TABLE_EMAIL.COL_IDEMAIL)
      	pclDossier est un cDossier dynamique = pclCompte.getDossier(TABLEH_COMPTES.COL_IDDOSSIER)
      	pclEmail est un cEmail = pclDossier.getEmail(TABLE_EMAIL.COL_IDEMAIL)
      	
      	si pclEmail.changeEtatLu(bLu)<>bLu ALORS
      		nNbChangements++
      	FIN
      FIN
      
      si bLu alors
      	ChangeNombreNonLusTableHierarchique(TABLEH_COMPTES,TABLEH_COMPTES[TABLEH_COMPTES].COL_NB_NON_LUS-nNbChangements)
      sinon
      	ChangeNombreNonLusTableHierarchique(TABLEH_COMPTES,TABLEH_COMPTES[TABLEH_COMPTES].COL_NB_NON_LUS+nNbChangements)
      fin
      
      FinTimerSys(TIMER_SYS_PASSE_LU)
      
      renvoyer nNbChangements
