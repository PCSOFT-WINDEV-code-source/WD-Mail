   -
     name : emailReleve
     procedure_id : 1969707010287560980
     type_code : 14
     code : |1-
      // Résumé : Relève les emails pour le compte courant
      // Syntaxe :
      //emailReleve (<tabCompte> est tableau)
      //
      // Paramètres :
      //	tabCompte (tableau) : liste des comptes à relever
      // Valeur de retour :
      // 	Aucune
      
      PROCÉDURE emailReleve(tabCompte est un tableau de cCompte dynamique)
      
      arreteNotifieur()
      GR_OPERATION_EN_COURS..visible = vrai
      COL_WDMail.gbDemandeAbandonOperation = faux
      
      // Variable
      pclDossier	est un cDossier dynamique
      pclCompte	est un cCompte dynamique
      nNbEmail	est un entier
      
      // Initialisation
      tabTP est un tableau de TâchesParallèles
      POUR TOUT ELEMENT pclCompte DE tabCompte
      	// Relève les emails
      	pclCompte.CallbackAvancement = callbackAvancementOperation
      	tp est une TâcheParallèle = TâcheParallèleExécute(pclCompte.emailRecupere,(),tpoCopieLégèreContexteHFSQL)
      	tabTP.ajoute(tp)
      	TâcheParallèleExécuteAprès(tp,()=>{ nNbEmail += MaTâcheParallèlePrécédente.ValeurRenvoyée })
      FIN
      
      TâcheParallèleExécuteAprèsToutes(tabTP,finReleve,(),tpoThreadPrincipal)
      
      procedure interne finReleve()
      
      	// Actualise l'affichage
      	actualiseListeDossier()
      	SI nNbEmail > 0 ALORS
      		
      		// Se positionne sur la boite de réception du compte courant
      		pclDossier = pclCompte.getDossierReception()
      		
      		// Cherche le dossier dans la table hiérarchique et se positionne
      		TableSelectPlus(TABLEH_COMPTES, TableCherche(COL_IDDOSSIER,pclDossier.IDDossier,Vrai))
      		
      		// Action de sélection
      		selectionTableHierarchique()
      		
      		si nNbEmail=1 ALORS
      			message("Synchronisation terminée, un nouveau message")
      		sinon
      			message("Synchronisation terminée, [%nNbEmail%] nouveaux messages")
      		FIN
      	sinon
      		message("Synchronisation terminée")
      	FIN
      	
      	GR_OPERATION_EN_COURS..Visible = faux
      	demarreNotifieur()
      fin
