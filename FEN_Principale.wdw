   -
     name : actualiseTitre
     procedure_id : 1155964785497218080
     type_code : 14
     code : |1+
      procédure actualiseTitre()
      
      pclCompte est un cCompte dynamique = ccompte.s_GetCompte(TABLEH_COMPTES.COL_IDCOMPTE)
      si pclCompte<>null alors
      	pclDossierReception	est un cDossier dynamique	= pclCompte.getDossierReception()
      	SI pclDossierReception<>Null ALORS
      		FEN_Principale..Titre	= ChaîneConstruit("%1 - %2 %3/%4", ProjetInfo(piNomProjet), pclCompte.Nom, pclDossierReception.NbNonLus, pclDossierReception.emaiNbTotal())
      	FIN
      sinon
      	FEN_Principale..Titre = ProjetInfo(piNomProjet)
      fin
      
