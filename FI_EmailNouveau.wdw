     -
       name : BTN_ENVOYER
       identifier : 0x2faa9db33215256b
       internal_properties : 
       properties :
        x : 623
        y : 693
        width : 105
        height : 30
       type : 4
       code_elements :
        type_code : 4
        p_codes :
         -
           internal_properties : CAAAAAgAAAANK99aT578pqrY5vHKk6We4QlF1n4phI60i5kiJ2ajIXqY8ANw+K+Ev0qF5XyF7/X5nkR47wx/A9HQAdXUtV09qS9IbkPPF7nTWSaUTtcQidvXNTL45w==
           code : |1+
            // Vérifications
            SI COMBO_EXP = -1 ALORS
            	Erreur(<§$0000§>)
            	RepriseSaisie(COMBO_EXP)
            FIN
            
            SI SAI_DEST~="" _et_ SAI_COPIE~="" _et_ SAI_COPIE_CACHEE~="" alors
            	Erreur(<§$0001§>)
            	RepriseSaisie(SAI_DEST)	
            FIN
            
            // Vérifie les adresses emails de la table
            sEmailsInvalides est une chaine
            pour toute chaine semail de SAI_DEST separee par rc
            	SI EmailVérifieAdresse(cEmail::s_emailExtrait(semail),emailValideSyntaxe) = emailAdresseSyntaxeIncorrecte ALORS
            		sEmailsInvalides += [rc] + semail
            	FIN
            FIN
            POUR TOUTE CHAÎNE semail DE SAI_COPIE SÉPARÉE PAR RC
            	SI EmailVérifieAdresse(cEmail::s_emailExtrait(sEmail),emailValideSyntaxe) = emailAdresseSyntaxeIncorrecte ALORS
            		sEmailsInvalides += [rc] + sEmail
            	FIN
            FIN
            POUR TOUTE CHAÎNE semail DE SAI_COPIE_CACHEE SÉPARÉE PAR RC
            	SI EmailVérifieAdresse(cEmail::s_emailExtrait(sEmail),emailValideSyntaxe) = emailAdresseSyntaxeIncorrecte ALORS
            		sEmailsInvalides += rc + sEmail
            	FIN
            FIN
            
            si sEmailsInvalides<>"" alors
            	Erreur(ChaîneConstruit(<§$000f§>,sEmailsInvalides))
            	RepriseSaisie(SAI_DEST)
            FIN
            
            SI EDHTML_EMAIL ~= "" ALORS
            	Erreur(<§$0002§>)
            	RepriseSaisie(EDHTML_EMAIL)
            FIN
            
            pclCompte		est un cCompte dynamique	= cCompte::s_GetCompte(COMBO_EXP..Valeur)
            pclBrouillon	est un cDossier dynamique	= pclCompte.getDossierBrouillon()
            
            // Construction de l'email
            clEmail			est un cEmail dynamique		= construitEmail(pclCompte,(gsModeRéponse=cEmail.MODE_EDITION ? gpclEmailBase.IDEmail SINON -1))
            
            // Sauvegarde l'email dans brouillon en attente de l'envoi
            si pclBrouillon<>null alors
            	pclBrouillon.emailAjoute(clEmail)
            fin
            
            // Envoi l'email
            Ouvre(FEN_EmailEnvoi, clEmail)
            
            // Ferme l'onglet
            OngletFerme(FEN_Principale.ONG_WDMail)
            
            CAS EXCEPTION:
            Erreur(ExceptionInfo(errMessage))
            
            FIN:
            FEN_Principale.timerDoubleClic()
