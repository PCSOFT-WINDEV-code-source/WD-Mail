   -
     name : modifieSujet
     procedure_id : 2302902992438280148
     type_code : 14
     code : |1+
      procédure modifieSujet()
      
      // Il faut mettre à jour le libellé de l'onglet
      oVolet est un Champ <- VoletDuChamp(MaFenêtreInterne)
      
      si taille(SAI_SUJET)>40 alors
      	oVolet..Libellé = tronque(SAI_SUJET,37,nombreDeCaractèresAConserver)+"…"
      sinon
      	oVolet..Libellé = SAI_SUJET
      FIN
      
