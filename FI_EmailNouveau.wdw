     name : DnDAttachement
     procedure_id : 2302879512927622736
     type_code : 14
     code : |1-
      procédure DnDAttachement()
      
      bAjout est un booléen = faux
      nbAttach est un entier = ExplorerRécupère(_EVE.wParam)
      POUR i = 1 A nbAttach
      	bAjout = ajouteFichierDansTableAttache(ExplorerRécupère(_EVE.wParam, i))
      FIN
      
      si bAjout ALORS
      	actualiseEnteteTableAttachements()
      FIN
