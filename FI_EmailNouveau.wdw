  procedures :
   -
     name : initialiseFenetre
     internal_properties : CAAAAAgAAADoukUTH7As0sdw6ahHSftk/BN2AxbVycjZ/Mqk0oX8N44PJaUr35eY2eZbWaBkhg8sM8XgOkQBgP9njAYHZAP2U0HJUmIQv1IPWJ4Zb7YXjJkGZb1aVSL3Nd0C4SwD4B8Cz+lUOkAtXOXHr9cz
     procedure_id : 2281809553826981227
     type_code : 14
     code : |1+
      // Résumé : Initialise la fenêtre
      // Syntaxe :
      // initialiseFenetre ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      
      PROCÉDURE initialiseFenetre()
      
      // Initialisation de la liste des expéditeurs potentiels
      POUR TOUT ELEMENT pclCompte DE cCompte::s_getListeCompte()
      	ListeAjoute(COMBO_EXP, pclCompte.Nom + gLien(pclCompte.IDCompte))
      FIN
      
      // Sélectionne le premier par défaut
      SI gpclEmailBase = Null ALORS
      	SI gnIDCompte <> -1 ALORS
      		COMBO_EXP = gnIDCompte
      	SINON
      		ListeSelectPlus(COMBO_EXP, 1)	
      	FIN
      	// Sinon le compte associé à l'email
      SINON
      	COMBO_EXP = gpclEmailBase.IDCompte
      FIN
      
      
      // Par défaut, masque la table des pièces jointes
      TABLE_ATTACHE..Visible = faux
      		
      // Remplit les informations si un email est spécifié, dans le cas de réponse et transfert
      si gpclEmailBase <> Null ALORS
      	pclNouveauMessage est un cEmail dynamique = allouer un cEmail
      	gpclEmailBase.copie(pclNouveauMessage, gsModeRéponse)
      	
      	// Les destinataires
      	POUR TOUT ELEMENT sDestinataire DE pclNouveauMessage.Destinataire
      		SaisieAjouteJeton(SAI_DEST,sDestinataire)
      	FIN
      	
      	// Les destinataires Cc
      	POUR TOUT ELEMENT sDestinataire DE pclNouveauMessage.DestinataireCc
      		SaisieAjouteJeton(SAI_COPIE,sDestinataire)
      	FIN
      	
      	// Les destinataires Cci
      	POUR TOUT ELEMENT sDestinataire DE pclNouveauMessage.DestinataireCci
      		SaisieAjouteJeton(SAI_COPIE_CACHEE,sDestinataire)
      	FIN
      	
      	eTmp est un Email
      	eTmp.Source = pclNouveauMessage.EMLSource
      	EmailImporteSource(eTmp)
      	
      	selon gsModeRéponse
      		cas cemail.MODE_REPONDRE, cEmail.MODE_REPTOUS
      			// Contenu de l'email en mode réponse
      			EditeurHTMLDepuisEmail(EDHTML_EMAIL, eTmp,emailOptionRepondreAvant)
      			
      		cas cemail.MODE_TRANSFERT
      			// Contenu de l'email en mode transfert
      			EditeurHTMLDepuisEmail(EDHTML_EMAIL, eTmp,emailOptionTransféré)
      			
      		autre cas
      			// Mail vide ou contenu précédent pour les brouillons
      			EditeurHTMLDepuisEmail(EDHTML_EMAIL, eTmp)
      	FIN
      	
      	SAI_SUJET = pclNouveauMessage.Sujet
      	modifieSujet()
      	
      	
      	// Pièces jointes
      	SI pclNouveauMessage.NbPieceJointe > 0 ALORS 
      		POUR TOUT ELEMENT pclPJ, nRang DE pclNouveauMessage.getPiecesJointes()
      			// Si la pièce jointe ne provient pas du source HTML, on l'ajoute dans la table
      			// (dans le cas contraire, elle sera traitée par le champ d'édition HTML)
      			si ChaîneOccurrence(eTmp.HTML,"cid:"+pclPJ.IDAttache)=0 alors
      				TableAjouteLigne(TABLE_ATTACHE, pclPJ.NomFichier, pclPJ.TailleFichier, nRang)
      			fin
      		FIN
      		ActualiseEnteteTableAttachements()
      	FIN
      FIN
      
      
      
