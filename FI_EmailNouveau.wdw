   -
     name : actualiseEnteteTableAttachements
     procedure_id : 2302638466045506638
     type_code : 14
     code : |1+
      procédure actualiseEnteteTableAttachements()
      
      si TABLE_ATTACHE..Occurrence>0 ALORS
      	TABLE_ATTACHE..visible				= vrai
      	
      	TABLE_ATTACHE.COL_NomFichier..Titre	= TABLE_ATTACHE..Occurrence + (TABLE_ATTACHE..Occurrence=1 ? " pièce jointe" sinon " pièces jointes")
      	
      	nTailleTotale est un entier sur 8
      	pour TOUTE LIGNE DE TABLE_ATTACHE
      		nTailleTotale += COL_Taille_Totale
      	FIN
      	TABLE_ATTACHE.COL_Taille_Totale..Titre = TailleVersChaîne(nTailleTotale)
      	
      sinon
      	TABLE_ATTACHE..visible = faux
      FIN
