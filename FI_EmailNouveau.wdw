     name : ImporterTemplate
     procedure_id : 1170858125554585149
     type_code : 14
     code : |1+
      procédure ImporterTemplate()
      
      sFichier est une chaine = fSélecteur("","","Sélectionnez le fichier contenant le template à importer","Fichiers HTML (*.html, *.htm)"+tab+"*.html;*.htm","*.html;*.htm",fselOuvre)
      si fFichierExiste(sFichier) ALORS
      	EDHTML_EMAIL..RépertoireDeTravail	= fExtraitChemin(sFichier,fDisque+fRépertoire)
      	EDHTML_EMAIL						= fChargeBuffer(sFichier)
      FIN
      
