     name : focusOuverture
     internal_properties : CAAAAAgAAABu7yrhRHM93Rz75Z+N2CiAkQVmoSS0JMUiTX/XKp7oTxzHEcp46/bm3BQXuuZDqlDn7BjoSLQWQnlkJkawX1Adv8dzG58iy4pA63DnIG7Fej3RX66fw0/e
     procedure_id : 2303013025393389232
     type_code : 14
     code : |1+
      // Procédure automatique :
      // La procédure est exécutée manuellement, lors d'un appel dans le code, avec un différé de 50 centièmes de seconde
      // 
      procédure focusOuverture()
      
      // Donne le focus selon le mode d'ouverture de la fenêtre
      SELON gsModeRéponse
      	CAS cEmail.MODE_CREATION, cEmail.MODE_COPIE, cEmail.MODE_TRANSFERT
      		DonneFocus(SAI_DEST)
      	CAS cEmail.MODE_REPONDRE, cEmail.MODE_REPTOUS
      		DonneFocus(EDHTML_EMAIL)
      	cas cEmail.MODE_EDITION
      		si SAI_DEST~="" alors
      			DonneFocus(SAI_DEST)
      		sinon
      			DonneFocus(EDHTML_EMAIL)
      		FIN
      FIN
